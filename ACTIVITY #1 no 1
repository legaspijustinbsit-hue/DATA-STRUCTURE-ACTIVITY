#include <iostream>
Using namespace std;

int main() {
    // Problem 1: Print Numbers Using Two While Loops
    int i = 0;

    printf("Problem 1: Numbers from 0 to 10:\n");
    while (i <= 10) {
        printf("%d ", i);
        i++;
    }
    printf("\n");

    printf("Problem 1: Numbers from 10 to 0:\n");
    i = 10;
    while (i >= 0) {
        printf("%d ", i);
        i--;
    }
